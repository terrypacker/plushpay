<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <converter>
  <description>Converter for CurrecyType objects.</description>
  <converter-id>currencyTypeConverter</converter-id>
  <converter-class>com.payyourself.currency.type.CurrencyTypeConverter</converter-class>
 </converter>
 <converter>
  <converter-id>currencyConverter</converter-id>
  <converter-class>com.payyourself.currency.CurrencyConverter</converter-class>
 </converter>
 <converter>
  <converter-id>tradeConverter</converter-id>
  <converter-class>com.payyourself.trading.trade.TradeConverter</converter-class>
 </converter>
 <converter>
  <converter-id>currencyValueConverter</converter-id>
  <converter-class>com.payyourself.currency.CurrencyValueConverter</converter-class>
 </converter>
 <converter>
  <converter-id>currencyRateConverter</converter-id>
  <converter-class>com.payyourself.currency.rate.CurrencyRateConverter</converter-class>
 </converter>
 <converter>
  <converter-id>beneficiaryConverter</converter-id>
  <converter-class>com.payyourself.trading.beneficiary.BeneficiaryConverter</converter-class>
 </converter>
 <managed-bean>
  <managed-bean-name>userSession</managed-bean-name>
  <managed-bean-class>com.payyourself.userManagement.user.UserSession</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>user</property-name>
   <property-class>com.payyourself.userManagement.user.User</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <description>Bean to create a trade.</description>
  <managed-bean-name>createTrade</managed-bean-name>
  <managed-bean-class>com.payyourself.trading.trade.CreateTrade</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>currentRates</managed-bean-name>
  <managed-bean-class>com.payyourself.currency.rate.CurrentRates</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>registrationBean</managed-bean-name>
  <managed-bean-class>com.payyourself.registration.RegistrationBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>userPreferences</managed-bean-name>
  <managed-bean-class>com.payyourself.userManagement.user.UserPreferences</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>tradeGeneratorControl</managed-bean-name>
  <managed-bean-class>com.payyourself.trading.tradeGenerator.TradeGeneratorControl</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>tradeGeneratorAllTrades</managed-bean-name>
  <managed-bean-class>com.payyourself.trading.tradeGenerator.TradeGeneratorAllTrades</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>logfileFactory</managed-bean-name>
  <managed-bean-class>com.payyourself.log.LogfileFactory</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>userTradeHistory</managed-bean-name>
  <managed-bean-class>com.payyourself.trading.history.UserTradeHistory</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beneficiaries</managed-bean-name>
  <managed-bean-class>com.payyourself.trading.beneficiary.Beneficiaries</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>createTransaction</managed-bean-name>
  <managed-bean-class>com.payyourself.accounting.transaction.CreateTransaction</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>tradeGeneratorTraders</managed-bean-name>
  <managed-bean-class>com.payyourself.trading.tradeGenerator.TradeGeneratorTraders</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>tradeGeneratorDepositFundsTrades</managed-bean-name>
  <managed-bean-class>com.payyourself.trading.tradeGenerator.TradeGeneratorDepositFundsTrades</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>audBankSimulator</managed-bean-name>
  <managed-bean-class>com.payyourself.banking.au.AudBankSimulator</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <display-name>Trading Rules</display-name>
  <from-view-id>/trading_area/*</from-view-id>
  <navigation-case>
   <from-outcome>confirm_trade</from-outcome>
   <to-view-id>/trading_area/confirm_trade.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>trade_confirmed</from-outcome>
   <to-view-id>/trading_area/trade_confirmed.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>create_trade</from-outcome>
   <to-view-id>/trading_area/create_trade.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>user_home</from-outcome>
   <to-view-id>/user_area/user.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>history</from-outcome>
   <to-view-id>/trading_area/trade_history.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>preferences</from-outcome>
   <to-view-id>/user_area/preferences.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>New Member Registration</display-name>
  <from-view-id>/*</from-view-id>
  <navigation-case>
   <from-outcome>register</from-outcome>
   <to-view-id>/registration_area/register.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/home/start.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>admin</from-outcome>
   <to-view-id>/admin_area/administration.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>User Area</display-name>
  <from-view-id>/user_area/*</from-view-id>
  <navigation-case>
   <from-outcome>create_trade</from-outcome>
   <to-view-id>/trading_area/create_trade.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>history</from-outcome>
   <to-view-id>/trading_area/trade_history.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>preferences</from-outcome>
   <to-view-id>/user_area/preferences.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>user_home</from-outcome>
   <to-view-id>/user_area/user.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>beneficiaries</from-outcome>
   <to-view-id>/user_area/beneficiaries.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>Registration Area Rules</display-name>
  <from-view-id>/registration_area/*</from-view-id>
  <navigation-case>
   <from-outcome>registration_failed</from-outcome>
   <to-view-id>/registration_area/register_fail.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>registration_success</from-outcome>
   <to-view-id>/registration_area/register_success.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>Management Area</display-name>
  <from-view-id>/management_area/*</from-view-id>
  <navigation-case>
   <from-outcome>generator_settings</from-outcome>
   <to-view-id>/management_area/trade_generator_settings.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>management_home</from-outcome>
   <to-view-id>/management_area/management.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>free_traders</from-outcome>
   <to-view-id>/management_area/free_traders.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>all_traders</from-outcome>
   <to-view-id>/management_area/all_traders.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>deposit_funds_trades</from-outcome>
   <to-view-id>/management_area/deposit_funds_trades.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>all_trades</from-outcome>
   <to-view-id>/management_area/all_trades.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>Admin Area Rules</display-name>
  <from-view-id>/admin_area/*</from-view-id>
  <navigation-case>
   <from-outcome>view_logs</from-outcome>
   <to-view-id>/admin_area/viewlogs.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>admin_home</from-outcome>
   <to-view-id>/admin_area/administration.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>Accounting Area</display-name>
  <from-view-id>/accounting_area/*</from-view-id>
  <navigation-case>
   <from-outcome>accounting_home</from-outcome>
   <to-view-id>/accounting_area/accounting.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <validator>
  <validator-id>passwordValidator</validator-id>
  <validator-class>com.payyourself.validator.PasswordValidator</validator-class>
 </validator>
 <validator>
  <validator-id>usernameValidator</validator-id>
  <validator-class>com.payyourself.validator.UserNameValidator</validator-class>
 </validator>
 <validator>
  <validator-id>emailValidator</validator-id>
  <validator-class>com.payyourself.validator.EmailValidator</validator-class>
 </validator>
 <validator>
  <validator-id>routingNumberValidator</validator-id>
  <validator-class>com.payyourself.validator.RoutingNumberValidator</validator-class>
 </validator>
 <validator>
  <validator-id>floatingPointValidator</validator-id>
  <validator-class>com.payyourself.validator.FloatingPointValidator</validator-class>
 </validator>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  <locale-config>
   <default-locale>en</default-locale>
   <supported-locale>en</supported-locale>
  </locale-config>
  <message-bundle>com.payyourself.locale.Locale_en</message-bundle>
 </application>
 <lifecycle>
  <phase-listener id="HibernateRenderResponsePhaseListener">com.payyourself.persistence.HibernateRenderResponsePhaseListener</phase-listener>
  <phase-listener id="HibernateRestoreViewPhaseListener">com.payyourself.persistence.HibernateRestoreViewPhaseListener</phase-listener>
 </lifecycle>
</faces-config>
